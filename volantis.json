{
    "name": "Volantis.Common",
    "type": "package",
    "description": "A library containing common classes used by higher-level libraries and applications.",
    "developer": "Volantis Development",
    "website": "https://volantis.dev",
    "authors": [
        {
            "name": "Ben McClure",
            "email": "ben@volantisdev.com"
        }
    ],
    "dependencies": {
        "Volantis.Core": {
            "version": "^1.0",
            "libraries": "*"
        }
    },
    "devDependencies": {
        "Volantis.Test": "^1.0"
    },
    "libraries": {
        "Volantis.Base": {
            "path": "lib/Volantis.Base",
            "uses": [
                "Volantis.Core:Volantis.Core"
            ]
        },
        "Volantis.Component": {
            "path": "lib/Volantis.Component",
            "uses": [
                "Volantis.Config"
            ]
        },
        "Volantis.Config": {
            "path": "lib/Volantis.Config",
            "uses": [
                "Volantis.Container",
                "Volantis.File"
            ]
        },
        "Volantis.Container": {
            "path": "lib/Volantis.Container",
            "uses": [
                "Volantis.Data"
            ]
        },
        "Volantis.Data": {
            "path": "lib/Volantis.Data",
            "uses": [
                "Volantis.Base"
            ]
        },
        "Volantis.File": {
            "path": "lib/Volantis.File",
            "uses": [
                "Volantis.Data"
            ]
        },
        "Volantis.Query": {
            "path": "lib/Volantis.Query",
            "uses": [
                "Volantis.Base"
            ]
        }
    }
}
